{
  "$id": "blockchain-address.json",
  "title": "Blockchain address",
  "type": "object",
  "description": "An address on a blockchain",
  "properties": {
    "address": {
      "type": "string"
    },
    "tags": {
      "type": "array",
      "minItems": 1,
      "items": {
        "enum": ["eoa", "safe", "creator", "factory", "proxy", "contract"]
      }
    },
    "networks": {
      "type": "array",
      "minItems": 1,
      "items": {
        "enum": ["mainnet", "optimism"]
      }
    },
    "name": {
      "type": "string"
    }
  },
  "required": ["address", "tags", "networks"]
}
